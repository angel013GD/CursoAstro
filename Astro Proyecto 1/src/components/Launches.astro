---
//Poder ejecutar el js que queremos
import LaunchCard from '../components/CardLaunch.astro';
import {getLatestLaunches} from '../services/spacex'

const launches = await getLatestLaunches()
---

<div class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
    {
        launches.map(({id,links, details, flight_number: flightNumber, success})=>(
        <LaunchCard 
            id={id} 
            img={links.patch.small}
            details={details}
            flightNumber={flightNumber}
            success={success}
        />
        
                
        ))
    }
</div>